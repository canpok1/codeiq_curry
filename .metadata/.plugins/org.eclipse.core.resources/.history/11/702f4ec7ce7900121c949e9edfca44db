/**
 *
 */
package data;

/**
 * スパイスを表すクラスです。
 * @author tanabe
 *
 */
public class Spice {

	/**
	 * スパイスの種類です。
	 */
	private String type;

	/**
	 * 指定種類のスパイスを生成します。
	 * @param type 種類
	 */
	public Spice( String type )
	{
		if( type == null )
		{
			throw new NullPointerException();
		}

		this.type = type;
	}

	@Override
	public String toString()
	{
		return this.type.toString();
	}
}
